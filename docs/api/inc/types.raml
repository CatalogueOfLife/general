  type: object
  description: >
    An object which is part of a specific dataset.
  properties:
    datasetKey:
      type: integer
      description: Key to the source dataset this record belongs to
      required: true
VerbatimObject:
  type: DatasetObject
  properties:
    verbatimKey:
      type: integer
