      type: EstimateType
      description: the type of species estimate
      required: false
    referenceId:
      type: string
      description: the species estimate reference ID
    note:
      type: string
      description: remarks on the species estimate
      required: false
EstimateType:
  type: string
  enum: [DESCRIBED_SPECIES_LIVING, DESCRIBED_SPECIES_EXTINCT, ESTIMATED_SPECIES]
  description: >
    An enumeration of species estimate types
Taxon:
  type: VerbatimIdObject
  description: >
    A taxon concept, i.e. an accepted name. Not used for synonyms.
  properties:
    name:
      type: Name
      description: the name for this taxon
    status:
      type: TaxonomicStatusType
      description: the taxonomic status of the taxon
    parentKey:
      type: integer
      description: key to next higher taxon in the classification
      required: false
    accordingTo:
      type: string
      description: person who scrutinized the taxon. LTSSpecialist in the CoL submission format
      required: false
    accordingToDate:
      type: date-only
      description: date when the taxon was last scrutinized. LTSDate in the CoL submission format
      required: false
    fossil:
      type: boolean
      description: true if the taxon is known from fossil records. NULL if unknown.
      required: false
    recent:
      type: boolean
      description: true if the taxon is known from recent, i.e. holocene records. That includes organisms that died out in the last centuries. NULL if unknown.
      required: false
    lifezone:
      type: LifezoneType[]
      description: the environment the organism is known to live in.
      required: false
    referenceKeys:
      type: integer[]
      description: keys to references supporting the taxon concept.
      required: false
    speciesEstimate:
      type: integer
      description: Estimation of total number of species that have been described for this group.
      required: false
    speciesEstimateReferenceKey:
      type: integer
      description: Key to reference supporting the estimated number.
      required: false
    remarks:
      type: string
      description: notes for general remarks on the taxon
      required: false
  example: |
    {
        "key": 209379,
        "id": "Kew-114621",
        "datasetKey": 24,
        "name": {
            "key": 369744,
            "id": "Kew-114621",
            "datasetKey": 24,
            "scientificName": "Lithocarpus mariae",
            "rank": "species",
            "genus": "Lithocarpus",
            "specificEpithet": "mariae",
            "candidatus": false,
            "combinationAuthorship": {
                "authors": [
                    "Soepadmo"
                ]
            },
            "origin": "source",
            "type": "scientific",
            "parsed": true,
            "authorship": "Soepadmo"
        },
        "status": "accepted",
        "origin": "source",
        "parentKey": 209098,
        "accordingTo": "Govaerts R.",
        "fossil": false,
        "recent": true
    }
TaxonResultSet:
  type: ResultSet
  description: >
    A paging resultset for Taxa.
  properties:
    results:
      type: Taxon[]
      description: the resulting Taxa
TaxonReference:
