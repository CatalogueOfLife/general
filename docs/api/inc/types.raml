VerbatimRecord:
  type: DatasetObject
  description: >
    Verbatim version of the entire source record with issues flagged during processing.
  properties:
    key:
      type: integer
      description: Internal primary key
    line:
      type: integer
      description: The line number of the file this record originated from
    file:
      type: string
      description: The name of the file this record originated from
    type:
      type: string
      description: The rowType (class) term of the file this record originated from
    terms:
      type: object
      description: Map of DwC or ACEF terms to their string value
    issues:
      type: IssueType[]
      description: Set of issues spotted during processing
      required: false
  example: |
    {
        "key": 114621,
        "datasetKey": 24,
        "line": 4,
        "file": "AcceptedSpecies.txt",
        "type": "acef:AcceptedSpecies",
        "terms": {
            "acef:AuthorString": "Soepadmo",
            "acef:Family": "Fagaceae",
            "acef:AcceptedTaxonID": "Kew-114621",
            "acef:Phylum": "Tracheophyta",
            "acef:LTSDate": "Aug-2017",
            "acef:Sp2000NameStatus": "accepted name",
            "acef:LTSSpecialist": "Govaerts R.",
            "acef:HasModern": "1",
            "acef:SpeciesEpithet": "mariae",
            "acef:Kingdom": "Plantae",
            "acef:Order": "Fagales",
            "acef:Genus": "Lithocarpus",
            "acef:HasPreHolocene": "0",
            "acef:Class": "Magnoliopsida",
            "acef:IsExtinct": "0"
        },
        "issues": [
            "according to date invalid",
            "escaped characters"
        ]
    }
VerbatimRecordResultSet:
  type: ResultSet
  description: >
    A paging resultset for VerbatimRecords.
  properties:
    results:
      type: VerbatimRecord[]
      description: the resulting VerbatimRecords
VernacularName:
