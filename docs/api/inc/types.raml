                "specificEpithet": "mariae",
                "candidatus": false,
                "combinationAuthorship": {
                    "authors": [
                        "Soepadmo"
                    ]
                },
                "origin": "source",
                "type": "scientific",
                "parsed": true,
                "authorship": "Soepadmo"
